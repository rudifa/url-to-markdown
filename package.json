{
  "name": "url-to-markdown",
  "version": "0.9.6",
  "description": "A Logseq plugin that automatically converts URLs to markdown with titles and favicons",
  "main": "dist/index.html",
  "type": "module",
  "author": "rudifa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rudifa/url-to-markdown.git"
  },
  "homepage": "https://github.com/rudifa/url-to-markdown",
  "bugs": {
    "url": "https://github.com/rudifa/url-to-markdown/issues"
  },
  "keywords": [
    "logseq",
    "plugin",
    "markdown",
    "url",
    "favicon",
    "metadata"
  ],
  "scripts": {
    "build": "./scripts/build.sh main",
    "build:visual-test": "./scripts/build.sh visual-test",
    "serve:visual-test": "npm run build:visual-test && cd dist-dev && python3 -m http.server 3003 --bind 127.0.0.1",
    "serve:visual-test:auto": "./scripts/dev-server-auto.sh",
    "serve:visual-test:dev": "./scripts/dev-server.sh",
    "dev:hmr": "./scripts/hmr-server.sh",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:web": "RUN_WEB_TESTS=true vitest run"
  },
  "dependencies": {
    "@logseq/libs": "^0.2.1"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.1.0",
    "@vitest/ui": "^3.2.4",
    "esbuild": "^0.25.8",
    "nodemon": "^3.1.10",
    "typescript": "^5.0.0",
    "undici": "^7.13.0",
    "vitest": "^3.2.4"
  },
  "logseq": {
    "id": "_ollalogvj",
    "title": "URL to Markdown",
    "icon": "favicon.svg"
  }
}
